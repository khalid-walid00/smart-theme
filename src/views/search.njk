{% extends "layouts/layout.njk" %}
{% from "common/components/swipper/index.njk" import Homeswipper,ProductCardswipper,opinionsswipper %}
{% from "common/components/headers/index.njk" import titleSection %}
{% from "common/components/cards/index.njk" import cardSection1,cardOptions %}
{% from "common/components/operations/index.njk" import showMore ,navogationLink%}

{% block content %}
<div class="main-bg pb-10">
    <div class="flex justify-center ">
        <div class="md:w-10/12 w-11/12">
            <div class="my-20">
                {{navogationLink(firstText="الرئيسية",firstLink="/",secondText=page.title,secondLink="#")}}
            </div>
            <div class=" my-10 relative">
                <img class="w-full" src={{"images/pages.webp" | assets}}>
            </div>
            <div class=" grid md:grid-cols-3  grid-cols-2 gap-3">

                {% for product in context.products %}
                    {{cardSection1(category=product.title,image=product.images[0].imageUrl, price=product.price, discount="67", priceBeforeDiscount="67", rating=5)}}
                {% endfor %}
     
            </div>
        </div>
    </div>
</div>
{% endblock %}





































{% extends "layouts/layout.njk" %}
{% block content %}
<div class="py-[60px]" x-data="CartItemList()" x-init="init()">
    <div class=" flex flex-col gap-8 md:gap-10 container md:px-4">
        <div class=" flex flex-col gap-2">
            <h1 class=" font-bold text-2xl text-customBlack">{{context.page.title}}</h1>
            <p class=" text-subColor">{{context.page.description}}</p>
        </div>
        <div class=" flex md:flex-row flex-col gap-[20px] lg:gap-[43px]">
            <div style="border-radius:  20px;" class=" bg-white initial-scroll  md:w-2/3   flex flex-col gap-6">
                <input type="hidden" name="cartItem" value={{item._id}}>
                <template x-for="item in cart.items" :key="index">

                    <div class="w-full relative  gap-8  py-4 flex items-center  rounded-lg
                          shadow-[0px_0px_25px_#D7D7D780] p-3 md:p-[20px]">
                        <div class=" w-1/2  flex gap-4  ">
                            <div class=" w-max">
                                <img :src="item.product.images[0].imageUrl" class="w-[80px] h-[80px] md:w-[60px] md:h-[60px]"
                                style=" object-fit: contain; border-radius: 50%;" alt="">
                            </div>
                        
                        </div>
                        <div class=" w-1/2 grid grid-cols-1 md:grid-cols-4 md:w-full gap-2 ">
                            <div class="md:w-[113px]  flex flex-col justify-end items-end md:items-center md:justify-center">
                                <div class="text-customBlack w-max">
                                    <span
                                        x-text="item.product.title.length > 18 ? item.product.title.slice(0, 18) + '...' : item.product.title"></span>
                                </div>
                                <div class="text-[#929292]">
                                    <template x-if="item.properties && item.properties.length > 0">
                                        <template x-for="prop in item.properties" :key="prop.property">
                                            <span class="text-nowrap">
                                                <span x-text="prop.property"></span> : <span
                                                    x-text="prop.value.title"></span>
                                            </span>
                                        </template>
                                    </template>
                                </div>
                            </div>
                            <div class="text-customBlack flex  w-full justify-end md:justify-self-end font-[600] place-self-center">
                                <div class=" flex items-center gap-1">
                                    <p x-text="item.totalPriceForItem"
                                        class=" text-nowrap font-bold text-[#434A5B]">
                                    </p>
                                    <p x-show="+item.discount > 0" x-text="item.totalPriceForItemBeforeDiscount"
                                        :class="item.discount > 0 ? 'line-through' : ''"
                                        class=" text-nowrap  text-sm text-[#929A9A]">
                                    </p>
                                </div>
                            </div>
                            <div class=" flex justify-end">

                                <qumra-quantity-input size="32px" :item="item._id" :value="item.quantity">
                                </qumra-quantity-input>
                            </div>
                            <div class=" flex absolute -top-2 left-0 justify-end gap-4">
                                <button @click="deleteItemCart(item._id)">
                                    <span
                                        class=" bg-[#F2F2F2] hover:scale-105 transition-all duration-300 rounded-full w-[32px] h-[32px] flex items-center justify-center ">
                                        <i @click="window.Qumra.cart.clearCartItem(item._id)"
                                            class="fi flex items-center cursor-pointer justify-center text-[18px] text-red-600 fi-rs-trash"></i>
                                    </span>
                                </button>
                            </div>
                        </div>

                    </div>
                </template>

            </div>

            <div class="md:w-1/3 flex flex-col items-center gap-6">
