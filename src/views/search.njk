<script>
    function CartItemList() {
        return {
            items: context.cart.items | dump | safe,
            addItemCart(items) {
                window.Qumra.events.on("cart::updated", (cart) => {
                    GlobalState().updateCartNumber(cart.itemsCount)
                })
            },
            deleteItemCart(index, newFruit) {
                this.items[index] = newFruit;
            },
            init() {
                window.addItemCart = this
                    .addItemCart
                    .bind(this);
            }
        }
    }
</script>
